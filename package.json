{
  "name": "tienda-panel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:prod": "cross-env NODE_ENV=production NEXT_PUBLIC_BUILD_ENV=production next build",
    "build:analyze": "cross-env ANALYZE=true NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && next export",
    "export:prod": "cross-env NODE_ENV=production NEXT_PUBLIC_BUILD_ENV=production next build",
    "clean": "rimraf .next out",
    "clean:win": "if exist .next rmdir /s /q .next && if exist out rmdir /s /q out",
    "serve": "npx serve out -p 3000",
    "serve:panel": "npx serve out -p 3000 -s --cors",
    "check-build": "ls -la out/",
    "check-build:win": "if exist out (dir out)",
    "deploy:check": "echo 'Verificando archivos para deploy...' && ls -la out/ && echo 'Archivos listos para subir a public_html/panel/'",
    "build:deploy": "npm run clean && npm run export:prod && npm run deploy:check"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@heroui/button": "^2.2.19",
    "@heroui/input": "^2.4.19",
    "@heroui/react": "^2.7.11",
    "@heroui/system": "^2.4.15",
    "@heroui/theme": "^2.4.15",
    "@tailwindcss/forms": "^0.4.0",
    "axios": "^1.9.0",
    "framer-motion": "^12.23.0",
    "install": "^0.13.0",
    "next": "^15.3.4",
    "npm": "^11.4.2",
    "primereact": "^10.9.5",
    "react": "^18.3.1",
    "react-countup": "^6.1.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^4.3.1",
    "react-responsive-modal": "^6.4.2",
    "recharts": "^2.15.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "eslint": "8.4.1",
    "eslint-config-next": "^13.0.5",
    "postcss": "^8.4.5",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.0.5"
  }
}